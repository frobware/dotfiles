# Switch to Emacs
gconftool-2 --type string --set /desktop/gnome/keybindings/custom1/name "Switch to Emacs"
gconftool-2 --type string --set /desktop/gnome/keybindings/custom1/action "wmctrl -x -a Emacs"
gconftool-2 --type string --set /desktop/gnome/keybindings/custom1/binding "<Shift><Alt>e"

# Switch to Terminal
gconftool-2 --type string --set /desktop/gnome/keybindings/custom2/name "Switch to Terminal"
gconftool-2 --type string --set /desktop/gnome/keybindings/custom2/action "wmctrl -x -a gnome-terminal"
gconftool-2 --type string --set /desktop/gnome/keybindings/custom2/binding "<Shift><Alt>n"

# Switch to Browser
gconftool-2 --type string --set /desktop/gnome/keybindings/custom3/name "Switch to Browser"
gconftool-2 --type string --set /desktop/gnome/keybindings/custom3/action "wmctrl -x -a google-chrome"
gconftool-2 --type string --set /desktop/gnome/keybindings/custom3/binding "<Ctrl><Shift><Alt>n"

gconftool-2 --type string --set /desktop/gnome/keybindings/custom4/name "Maximize Vertically"
gconftool-2 --type string --set /desktop/gnome/keybindings/custom4/action "wmctrl -r :ACTIVE: -b toggle,maximized_vert"
gconftool-2 --type string --set /desktop/gnome/keybindings/custom4/binding "<Ctrl><Shift><Alt>v"

# wmctrl -r :ACTIVE: -b toggle,maximized_vert
 